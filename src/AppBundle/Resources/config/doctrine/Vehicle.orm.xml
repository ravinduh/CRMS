<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Vehicle" table="vehicle">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="plate" type="string" column="plate" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fuel" type="string" column="fuel" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="transmission" type="string" column="transmission" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="600" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="false"/>
  </entity>
</doctrine-mapping>
